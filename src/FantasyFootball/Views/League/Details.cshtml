@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model FantasyFootball.Models.League

@Html.ActionLink("View free agents", "Index", "Player")

<h2>@Model.Name</h2>

@foreach(var team in Model.Teams)
{
   
    <h4>@Html.ActionLink(team.TeamName, "Index", "Team", new { id = team.TeamId })</h4>
    <h5>Total Points: @team.TotalPoints</h5>
    <p>Points this week: @team.WeekPoints</p>
}
@Html.ActionLink("Home", "Index")